<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Pierre's Vendor!</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="~/css/styles.css" />
  </head>
  <body>
    @using Vendor.Models;

    <h3>Here are all the orders for your company:</h3>

    <ol>
    @foreach (Order order in @Model["orders"])
    {
      <li><a href='/distributors/@Model["distributor"].Id/orders/@order.Id'>@order.Description</a></li>
    }
    </ol>

    <p><a href='/distributors/@Model["distributor"].Id/orders/new'>Add a new order</a></p>
    <p><a href='/distributors'>Return to distributor list</a></p>
    <p><a href='/'>Return to Main Page</a></p>
  </body>
</html>